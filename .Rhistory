dust_data_soup$Fine.dust2[dust_data_soup$Fine.dust >=81 & dust_data_soup$Fine.dust <=150] <- 'bad'
dust_data_soup$Fine.dust2[dust_data_soup$Fine.dust >=151 ] <- 'bad'
dust_data_soup
